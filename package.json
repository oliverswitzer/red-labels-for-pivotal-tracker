{
  "name": "wwltw-for-pivotal-tracker",
  "version": "1.0.0",
  "description": "This plugin helps users keep track of what they learned while working on stories in the backlog. It prompts the user for optional feedback on what they learned for every story they complete.",
  "main": "karma.conf.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "karma start --single-run",
    "package": "./scripts/update_environment_to_production.js && npm run webpack && ./scripts/package.js && ./scripts/reset_environment_to_development.js",
    "deploy": "./scripts/deploy.js",
    "package_and_deploy": "npm run package | tail -n 1 | npm run deploy",
    "watch": "webpack --progress --colors --watch",
    "webpack": "webpack --progress --colors",
    "karma": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oliverswitzer/wwltw-for-pivotal-tracker.git"
  },
  "author": "Eve Asher, Katie Hoffman, Shawna Mattison, Oliver Switzer, Sai To Yeung",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oliverswitzer/wwltw-for-pivotal-tracker/issues"
  },
  "homepage": "https://github.com/oliverswitzer/wwltw-for-pivotal-tracker#readme",
  "devDependencies": {
    "child_process": "^1.0.2",
    "dotenv-webpack": "^1.4.2",
    "fs-promise": "^2.0.0",
    "get-stdin": "^5.0.1",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "jsonfile": "^2.4.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "marked": "^0.3.6",
    "node-libcurl": "^1.1.0",
    "refresh-token": "^0.1.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "highlight.js": "^9.10.0",
    "jquery": "^3.1.1",
    "moment": "^2.17.1",
    "semantic-ui-dimmer": "^2.2.9",
    "semantic-ui-dropdown": "^2.2.9",
    "semantic-ui-modal": "^2.2.9",
    "semantic-ui-transition": "^2.2.9",
    "showdown": "^1.6.4"
  }
}
