{
  "name": "What We Learned This Week for Pivotal Tracker",
  "version": "1.3.5",
  "version_name": "beta",
  "manifest_version": 2,
  "author": "Oliver Switzer, Sai To Yeung, Shawna Mattison",
  "description": "A better workflow for writing What We Learned This Week learnings",
  "homepage_url": "https://github.com/oliverswitzer/red-labels-for-pivotal-tracker",
  "icons": {},
  "permissions": [
    "http://www.pivotaltracker.com/*",
    "https://www.pivotaltracker.com/*",
    "storage",
    "alarms"
  ],
  "options_page": "src/options/index.html",
  "short_name": "WWLTW",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "http://www.pivotaltracker.com/*",
        "https://www.pivotaltracker.com/*"
      ],
      "js": [
        "vendor/moment/moment.min.js",
        "vendor/jquery/jquery-3.1.1.min.js",
        "vendor/semantic-ui/dropdown.min.js",
        "vendor/semantic-ui/modal.min.js",
        "vendor/semantic-ui/dimmer.min.js",
        "vendor/semantic-ui/transition.min.js",
        "src/content_scripts/view/wwltw_modal.js",
        "src/content_scripts/view/tagList.js",
        "src/content_scripts/view/story_listener.js",
        "src/content_scripts/utilities/description_builder.js",
        "src/content_scripts/utilities/pivotal_tracker_api_client.js",
        "src/content_scripts/utilities/project_id_provider.js",
        "src/content_scripts/utilities/fetch_wrapper.js",
        "src/content_scripts/utilities/date_wrapper.js",
        "src/content_scripts/utilities/story_title_provider.js",
        "src/content_scripts/utilities/analytics_wrapper.js",
        "src/content_scripts/repositories/wwltw_repository.js",
        "src/content_scripts/alarm_creator.js",
        "src/content_scripts/use_cases/add_learning_to_story_description.js",
        "src/content_scripts/use_cases/find_or_create_wwltw_story.js",
        "src/content_scripts/main.js"
      ],
      "css": [
        "vendor/semantic-ui/button.min.css",
        "vendor/semantic-ui/container.min.css",
        "vendor/semantic-ui/dimmer.min.css",
        "vendor/semantic-ui/dropdown.min.css",
        "vendor/semantic-ui/form.min.css",
        "vendor/semantic-ui/grid.min.css",
        "vendor/semantic-ui/header.min.css",
        "vendor/semantic-ui/icon.min.css",
        "vendor/semantic-ui/input.min.css",
        "vendor/semantic-ui/label.min.css",
        "vendor/semantic-ui/modal.min.css",
        "vendor/semantic-ui/site.min.css",
        "vendor/semantic-ui/transition.min.css",
        "css/inject/inject.css"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "src/background/first_run.js",
      "src/background/alarm_manager.js"
    ]
  }
}
